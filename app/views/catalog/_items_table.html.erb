<div class="items">
  <% if document.fulltext_urls.any? || document.findingaid_urls.any? %>
    <h3 class="online-header"> Access Online <i class="fa fa-laptop" aria-hidden="true"></i></h3>
    <div class="fulltext-url">
      <ul>
      <% document.fulltext_urls.each do |url| %>
        <li><i class="fa fa-square"></i> <%= link_to_primary_url(url) %></li>
      <% end %>
      <% document.findingaid_urls.each do |url| %>
        <li><i class="fa fa-square"></i> <%= link_to_primary_url(url) %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <% unless document.holdings.empty? %>
    <h3>Access at the Library <i class="fa fa-map-marker" aria-hidden="true"></i></h3>
    <% if context == 'index' %>
      <%= render partial: "items_table_index", locals: { document: document} %>
    <% else %>
      <%= render partial: "items_table_show", locals: { document: document } %>
    <% end %>
  <% end %>
</div>
